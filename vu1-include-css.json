{
    "app_id": "com.ibm.connections.vu1css",
    "name": "Visual Update 1 - Direct CSS",
    "title": "Visual Update 1 - Direct CSS",
    "description": "Customizer extension to re-theme Connections for Pink.",
    "services": [
        "Customizer"
    ],
    "extensions": [
        {
            "name": "Blogs",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "include-files": [
                    "css/blogs.css"
                ],
                "include-repo": {
                    "name": "periscope"
                }
            },
            "path": "blogs",
            "application": "Visual Update 1 for IBM Connections"
        },
        {
            "name": "Communities Landing",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "match": {
                    "url": "ownedcommunities|mycommunities|followedcommunities|communityinvites|allcommunities|trashedcommunities|communitycreate|communityedit"
                },
                "include-files": [
                    "css/communities.css"
                ],
                "include-repo": {
                    "name": "periscope"
                }
            },
            "path": "communities",
            "application": "Visual Update 1 for IBM Connections"
        },
        {
            "name": "Files",
            "type": "com.ibm.customizer.ui",
